{
  "name": "create-windowless-app-template",
  "version": "11.0.3",
  "private": true,
  "main": "_build/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "type-check": "tsc --build tsconfig.json",
    "prewebpack": "rimraf build && rimraf dist",
    "webpack": "webpack",
    "prebuild": "npm run check-node-version",
    "build": "npm run type-check && npm run webpack && npm run node-sea",
    "check-node-version": "ts-node -e \"require(\"\"./utils/checkNodeVersion\"\").checkNodeRuntimeVersion()\"",
    "check-msbuild": "ts-node -e \"require(\"\"./launcher/launcherCompiler\"\").checkMsbuildInPath(true)\"",
    "rebuild-launcher": "msbuild launcher/launcher.csproj",
    "prenode-sea:build-blob": "rimraf _blob && mkdir _blob",
    "node-sea:build-blob": "node --experimental-sea-config sea-config.json",
    "node-sea:copy-node": "node -e \"require('fs').copyFileSync(process.execPath, 'dist/create-windowless-app-template.exe')\"",
    "node-sea:unsign": "signtool remove /s dist/create-windowless-app-template.exe || echo Warning: signtool not found in \"Path\"",
    "node-sea:inject-blob": "postject dist/create-windowless-app-template.exe NODE_SEA_BLOB _blob\\sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2",
    "node-sea:sign": "signtool sign /fd SHA256 dist/create-windowless-app-template.exe",
    "node-sea": "npm run node-sea:build-blob && npm run node-sea:copy-node && npm run node-sea:unsign && npm run node-sea:inject-blob",
    "prepare": "git config --get core.hookspath || husky",
    "pre-commit": "git diff HEAD --exit-code --stat launcher/* || npm run check-msbuild && npm run rebuild-launcher && git add resources/bin/create-windowless-app-template-launcher.exe"
  },
  "dependencies": {
    "node-notifier": "10.0.1",
    "winston": "3.13.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/cross-spawn": "6.0.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.12",
    "@types/node-notifier": "8.0.5",
    "@types/winston": "2.4.4",
    "copy-webpack-plugin": "12.0.2",
    "cross-spawn": "7.0.3",
    "fs-extra": "11.2.0",
    "husky": "9.1.2",
    "jest": "29.7.0",
    "postject": "1.0.0-alpha.6",
    "rimraf": "5.0.9",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4"
  }
}
